{
  "$schema": "https://json.schemastore.org/package",
  "name": "@pnpm-monorepo/react-sample-lib",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./build/*.js",
      "types": "./build/*.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf ./build && tsc --build --force",
    "dev": "rimraf ./tsconfig.tsbuildinfo && tsc --watch",
    "test": "pnpm test:lint && pnpm test:types && pnpm test:unit",
    "test:lint": "eslint .",
    "test:types": "tsc --noEmit",
    "test:unit": "echo \"Warning: no test specified\" && exit 0"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "^1.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vanilla-extract/css": "^1.13.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
